#!/usr/bin/env ruby
INPUT = DATA.read.chars

def marker(char_size)
  one_less_char_size = char_size - 1

  INPUT.each_with_index do |char, i|
    next if i < one_less_char_size

    if INPUT[i - one_less_char_size..i].uniq.size == char_size
      return i + 1
    end
  end
end

puts marker(4)
puts marker(14)

__END__
